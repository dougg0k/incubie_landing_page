{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES2020",
		"jsx": "react",
		"lib": ["dom", "ES2020"],
		"strict": true,
		"noEmit": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"noUnusedLocals": false
	},
	"exclude": ["node_modules", "public", ".cache"],
	"include": ["src/**/*", "*.js"]
}
